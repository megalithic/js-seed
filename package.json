{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/js/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "reactify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "./test/helpers/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testDirectoryName": "test",
    "testPathIgnorePatterns": [
      "./node_modules",
      "./test/helpers"
    ],
    "moduleFileExtensions": [ "js" ]
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "connect": "~2.14.3",
    "es5-shim": "^4.0.1",
    "gulp": "~3.6.0",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-jasmine": "^0.3.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-react": "^0.5.0",
    "gulp-util": "~2.2.14",
    "jest": "^0.1.37",
    "jest-cli": "^0.1.18",
    "phantomjs": "^1.9.7-15",
    "pretty-hrtime": "~0.2.1",
    "react-tools": "^0.11.1",
    "reactify": "^0.14.0",
    "reflux": "^0.1.6",
    "superagent": "^0.18.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "gulp-jest": "git+https://github.com/Dakuan/gulp-jest"
  },
  "dependencies": {
    "jquery": "~2.1.0",
    "es6-promise": "^1.0.0",
    "react": "0.11.1"
  }
}
