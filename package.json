{
  "name": "js-seed",
  "version": "0.1.0",
  "description": "Starter repo for JS development (geared towards React/Compass)",
  "repository": {
    "type": "git",
    "url": "git://github.com/megalithic/js-seed.git"
  },
  "browser": {
    "plugin": "./src/js/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "reactify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "./test/helpers/preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ],
    "testDirectoryName": "test",
    "testPathIgnorePatterns": [
      "./node_modules/",
      "./test/helpers/"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "connect": "~2.14.3",
    "es5-shim": "^4.0.1",
    "event-emitter": "^0.3.1",
    "firebase-client": "git://github.com/tinj/firebase-client",
    "firebase-simple-login": "git://github.com/tinj/firebase-simple-login",
    "flux": "^1.0.2",
    "gulp": "~3.6.0",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-jasmine": "^0.3.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-plumber": "^0.6.4",
    "gulp-react": "^0.5.0",
    "gulp-ruby-sass": "*",
    "gulp-run": "^1.6.3",
    "gulp-sass": "^0.7.3",
    "gulp-shell": "^0.2.9",
    "gulp-stylus": "~1.3.0",
    "gulp-util": "~2.2.14",
    "jest": "^0.1.37",
    "jest-cli": "^0.1.18",
    "phantomjs": "^1.9.7-15",
    "pretty-hrtime": "~0.2.1",
    "react-dispatcher": "^0.2.0",
    "react-router": "^0.5.2",
    "react-tools": "^0.11.1",
    "reactify": "^0.14.0",
    "superagent": "^0.18.2",
    "underscore": "^1.6.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1"
  },
  "dependencies": {
    "jquery": "~2.1.0",
    "es6-promise": "^1.0.0",
    "react": "0.11.1"
  }
}
